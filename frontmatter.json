{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/pages/blog"
    }
  ],
  "frontMatter.taxonomy.alignFilename": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "blog",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "time",
          "name": "time",
          "type": "number"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "timestamp",
          "name": "timestamp",
          "type": "datetime"
        },
        {
          "title": "filename",
          "name": "filename",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "learning",
            "writing",
            "narratives"
          ]
        },
        {
          "title": "published",
          "name": "published",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true
}